{
  "name": "@gram/config",
  "private": true,
  "version": "3.1.2",
  "description": "Gram Configuration",
  "scripts": {
    "build": "npm run compile",
    "clean": "rimraf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "lint": "prettier --check .",
    "lint-fix": "prettier -l . --write",
    "debug": "NODE_ENV=development node --inspect=0.0.0.0 src/index.js",
    "test": "NODE_CONFIG_DIR=../config/ NODE_ENV=test jest --detectOpenHandles --no-cache --runInBand --logHeapUsage --forceExit",
    "snyk": "snyk test",
    "jest": "NODE_ENV=test jest --detectOpenHandles --no-cache",
    "sender": "NODE_ENV=development ts-node src/notifications/sender.ts",
    "gh": "ts-node src/packs/demo/test.ts"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klarna/gram.git"
  },
  "keywords": [
    "threat model",
    "appsec"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/klarna/gram/issues"
  },
  "homepage": "https://github.com/klarna/gram#readme",
  "engines": {
    "node": "16"
  },
  "dependencies": {
    "@gram/aws": "^3.1.2",
    "@gram/core": "^3.1.2",
    "@gram/magiclink": "^3.1.2",
    "@gram/svgporn": "^3.1.2",
    "@gram/threatlib": "^3.1.2"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2"
  },
  "optionalDependencies": {
    "dotenv": "^16.0.3"
  }
}
