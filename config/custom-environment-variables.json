{
  "origin": "ORIGIN",
  "data": {
    "_providers": {
      "postgres": {
        "host": "POSTGRES_HOST",
        "user": "POSTGRES_USER",
        "password": "POSTGRES_PASSWORD",
        "database": "POSTGRES_DB",
        "port": "POSTGRES_PORT"
      }
    }
  },

  "jwt": {
    "secret": {
      "auth": "AUTH_SECRET"
    }
  },

  "auth": {
    "providerOpts": {
      "github": {
        "appId": "GITHUB_APPID",
        "clientId": "GITHUB_CLIENTID",
        "clientSecret": "GITHUB_CLIENTSECRET",
        "privateKey": "GITHUB_PRIVATEKEY"
      }
    }
  },

  "notifications": {
    "providers": {
      "email": {
        "user": "EMAIL_USER",
        "password": "EMAIL_PASSWORD",
        "host": "EMAIL_HOST",
        "port": "EMAIL_PORT"
      }
    }
  }
}
